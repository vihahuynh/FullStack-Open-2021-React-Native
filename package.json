{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "chokidar": "^3.5.2",
    "dotenv": "^10.0.0",
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "formik": "^2.2.9",
    "graphql": "^16.0.1",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "^0.17.5",
    "react-router-native": "^5.2.1",
    "svgo": "^2.8.0",
    "yup": "^0.32.11",
    "@react-native-async-storage/async-storage": "~1.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.16.0",
    "@expo/webpack-config": "^0.16.6",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^8.0.0",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "jest-expo": "^43.0.1",
    "react-test-renderer": "^16.13.1"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
